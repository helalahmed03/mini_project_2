from tkinter import *

root = Tk()
root.geometry('300x300')
root.title("Melody")
root.iconbitmap(r'a2.ico')

text = Label(root, text='Lets play some music!')
text.pack()


def play_btn():
    print("Hey! This play button works pretty well!")


photo = PhotoImage(file='images/play1.png')
btn = Button(root, image=photo, command=play_btn)
btn.pack()

root.mainloop()
